	<mime-type type="application/x-sciprj">
		<comment xml:lang="en">SciDAVis project</comment>
		<comment xml:lang="de">SciDAVis-Projekt</comment>
		<comment xml:lang="fr">Projet Scidavis</comment>
		<comment xml:lang="pt_BR">Projeto do SciDAVis</comment>
